----GAMEPASS --BAT

local GetService=  game:GetService("Players")
    game:GetService("Players").LocalPlayer.Character.Bat.Attack:FireServer()

    local function task()
        local Event = LocalPlayer.Character.Bat.Attack:
        Event:FireServer()
    end
    
    
while true do

    for i = 1, 15 do
        task()
        wait()  
    end
    wait()  
end

